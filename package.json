{
  "name": "elevate-horizon-connect-as",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "inst": "npm install && cd server && npm install",
    "seed": "cd server && npm install && npm run seed",
